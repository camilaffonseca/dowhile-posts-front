{
  "name": "dowhile-posts-front",
  "version": "0.0.0",
  "repository": "https://github.com/camilaffonseca/dowhile-posts-front.git",
  "author": "Camila Fonseca <camilafonseca.ang7@gmail.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "reinstall": "yarn concurrently \"rm -rf ./node_modules\" \"rm -rf ./yarn.lock ./package-lock.json\" && yarn",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "commit": "npx git-cz",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "prettier": "prettier --loglevel warn --write .",
    "prettier:check": "prettier --check .",
    "eslint": "eslint . --fix --quiet",
    "eslint:check": "eslint .",
    "stylelint:check": "stylelint \"src/**/*.tsx\" \"src/**/*.ts\" --quiet"
  },
  "dependencies": {
    "@xstyled/styled-components": "^3.1.0",
    "@xstyled/system": "^3.1.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-icons": "^4.3.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vitejs/plugin-react": "^1.0.0",
    "commitlint": "^13.2.1",
    "concurrently": "^6.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "stylelint": "^14.0.1",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
